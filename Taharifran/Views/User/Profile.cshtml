@using Taharifran.Models
@model UserIndexViewModel
@{
    ViewBag.Title = "Profile";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Profile</title>
</head>
<body>
    <div>
        <table>
            @foreach (var user in Model.Users)
            {
                
            <dl class="dl-horizontal">
                <dt>Firstname:</dt>
                <dd>@user.Firstname</dd>

                
                <dt>Lastname</dt>
                <dd>@user.Lastname</dd>
                
            </dl>
            }
        </table>
    </div>
</body>
</html>
